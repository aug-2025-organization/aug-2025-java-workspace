public class RoomDemo {

	public static void main(String[] args) {
		
		int mark;
		mark = 60;
		
		Room room1;
		room1 = new Room(200, 100, 50);
		
//		room1.length = 200;
//		room1.width = 100;
//		room1.height = 50;
		
		int fArea = room1.calculateFloorArea();
		System.out.println("Floor Area : " + fArea);
		
		double pCost = room1.calculateWallPaintingCost(2);
		System.out.println("Paiting Cost : " + pCost);
		
		Room diningRoom = new Room(300, 200, 50);
//		diningRoom.length = 300;
//		diningRoom.width = 200;
//		diningRoom.height = 50;
		
		int fArea1= diningRoom.calculateFloorArea();
		System.out.println("Floor Area : " + fArea1);
		
		double pCost1 = diningRoom.calculateWallPaintingCost(2);
		System.out.println("Paiting Cost : " + pCost1);

		Room room3 = new Room(250, 150, 50, "Living Room");
		Room room4 = new Room(250, 150, 50, "Living Room");
		Room room5 = new Room(200, 100, 55, "Living Room");
		Room room6 = room3;
		
		System.out.println("room3 == room4 : " + (room3 == room4));
		System.out.println("room4 == room5 : " + (room4 == room5));
		System.out.println("room5 == room6 : " + (room5 == room6));
		
		
		
	}

}
