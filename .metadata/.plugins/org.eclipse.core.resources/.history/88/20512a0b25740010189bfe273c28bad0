
public class BoxDemo {

	public static void main(String[] args) {
		Box box1 = new Box(20, 15, 10);
		System.out.println(box1);
		
		BoxGenerics<Integer, String> box2 = new BoxGenerics<>(20, 15, 10, "white");
		System.out.println(box2);

		BoxGenerics<Double, String> box3 = new BoxGenerics<Double, String>(15.5, 10.5, 5.0, "black");
		System.out.println(box3);
		
		BoxGenerics<String> box4 = new BoxGenerics<String>("15", "10", "5");
	}

}
