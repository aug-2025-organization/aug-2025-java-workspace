public class Room {
	int length;
	int width;
	int height;

	int calculateFloorArea() {
		int floorArea = length * width;
		return floorArea;
	}

	double calculateWallPaintingCost(double paintingRate) {
		int wallArea = 2 * height * (length + width);
		double paintingCost = wallArea * paintingRate;
		return paintingCost;
	}
}
