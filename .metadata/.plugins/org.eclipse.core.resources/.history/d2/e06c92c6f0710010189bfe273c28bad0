import javax.swing.plaf.synth.SynthOptionPaneUI;

public class OperatorsDemo {

	public static void main(String[] args) {

		// & | << >> >>>
		int num1 = 5;
		int num2 = 3;
		int num3 = num1 | num2;
		// 0000 0101 
		// 0000 0011
		// 0000 0001
		// 0000 0111
		System.out.println(num3);

		int num4 = 3 << 2;
		// 0000 0011
		// 0000 0110
		// 0000 1100
		System.out.println(num4);
		int num5 = 5 >> 2
		// 0000 0101
		// 0000 0010
		// 0000 0001
		System.out.println(num5);	
				
	}

}
