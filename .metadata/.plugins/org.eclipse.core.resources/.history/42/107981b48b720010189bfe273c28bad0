public class RoomDemo {

	public static void main(String[] args) {
		
		int mark;
		mark = 60;
		
		Room room1;
		room1 = new Room();
		
		room1.length = 200;
		room1.width = 100;
		room1.height = 50;
		
		int fArea = room1.calculateFloorArea();
		System.out.println("Floor Area : " + fArea);
		
		double pCost = room1.calculateWallPaintingCost(2);
		System.out.println("Paiting Cost : " + pCost);
		
		Room diningRoom = new Room();
		diningRoom.length = 300;
		diningRoom.width = 200;
		diningRoom.height = 50;
		
		int fArea1= diningRoom.calculateFloorArea();
		System.out.println("Floor Area : " + fArea1);
		
		double pCost1 = diningRoom.calculateWallPaintingCost(2);
		System.out.println("Paiting Cost : " + pCost1);

	}

}
